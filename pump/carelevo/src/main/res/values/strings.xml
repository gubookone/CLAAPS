    <string name="carelevo_patch_attach_step1_title">Disinfect Patch Site</string>
    <string name="carelevo_patch_attach_step1_desc">Clean thoroughly with an alcohol swab.</string>
    <string name="carelevo_patch_attach_step2_title">Remove Backing Tape</string>
