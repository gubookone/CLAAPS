    <string name="carelevo_patch_needle_insertion_step2_title">Insert Needle</string>
    <string name="carelevo_patch_needle_insertion_step2_desc">Press the applicator push button all the way down. A beep will sound upon successful insertion.</string>
    <string name="carelevo_patch_needle_insertion_desc_warning">Press and hold for 10 seconds.</string>

    <string name="carelevo_dialog_patch_needle_check_title">Needle not inserted?</string>
    <string name="carelevo_dialog_patch_needle_check_desc">Press the applicator push button all the way down.\nYou will hear a beep sound upon successful insertion.\nTap the <b>[Check Insertion]</b> button below.</string>
